{
    "name": "npub/gos",
    "type": "library",
    "description": "SNILS validation and formatting class for PHP.",
    "keywords": ["СНИЛС", "SNILS", "Doctrine", "PHP", "validate", "ORM", "Entity", "EntityType"],
    "homepage": "https://github.com/npub/gos",
    "license": "MIT",
    "authors": [
        {"name": "Alexander Vasilyev", "email": "av@zbox.ru"}
    ],
    "require": {
        "php": "^8"
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0",
        "doctrine/orm": "*"
    },
    "suggest": {
        "doctrine/orm": "For using objects as Entity field type."
    },
    "autoload": {
        "psr-4": { "Npub\\Gos\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Npub\\Gos\\Test\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml"]
    }
}
